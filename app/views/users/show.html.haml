= render partial: "audios/header-sidebar"
.yohaku
#mainadd.main1
  .usershow
    .usershow__top
      -if @user.backgroundimage.present?
        = image_tag("#{@user.backgroundimage}",class:"userheaderimage")
      -else
        = image_tag("icon-1328421_1920.jpg",class:"userheaderimage")
    .usershow__userimage
      -if @user.userimage.present?
        = image_tag("#{@user.userimage}",class:"usericonimage")
      -else
        = image_tag("member_photo_noimage.png",class:"usericonimage")
      %p
        = @user.nickname
    -if @user == current_user 
      .usershow__edit
        = link_to "プロフィールを編集する","/users/#{current_user.id}/edit",class:"edit"
  .userprofile
    %p#profile プロフィール
    %p
      -if @user.introduction.present?
        = @user.introduction
      - else
        なし
  .usershow2
    .usershow2__bar1{onclick:"Usershow2Bar1()"}
      = @user.nickname
      さんの投稿
      %i#toukouyajirushi2.fas.fa-chevron-down
    #usershow2bar1.display_none
      -@audios.each do |audio|
        .music-form
          -if audio.image.present?
            = link_to image_tag(audio.image.url,class:"musicview1"),"/audios/#{audio.id}"
          -else
            = link_to image_tag("logo.jpg",class:"musicview1"),"/audios/#{audio.id}"          
          %li 
            = audio.title
          %li 
            = audio.artistname
          %li 
            = audio.updated_at
          %li 100いいね
